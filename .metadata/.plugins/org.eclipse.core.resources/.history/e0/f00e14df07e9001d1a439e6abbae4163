package principal;

import java.util.ArrayList;

public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String [] arrayEstatico = new String [5];
		arrayEstatico[0] = "Goku";
		arrayEstatico[1] = "Maria"; 
		arrayEstatico[2] = "Joao";
		arrayEstatico[3] = "Eric"; 
		arrayEstatico[4] = "Greice";
		
		
		System.out.println(arrayEstatico[0]);
		
		ArrayList<String> arrayDinamico = new ArrayList <String>();
		arrayDinamico.add("andrey0");
		arrayDinamico.add("Maria");
		
		for (int indice = 0; indice < arrayEstatico.length; indice++) {
			System.out.println(arrayEstatico[indice]);
		}
		
		System.out.println(arrayDinamico.get(1));
		//for (int indice = 0; indice < arrayDinamico)
		
	}

}
