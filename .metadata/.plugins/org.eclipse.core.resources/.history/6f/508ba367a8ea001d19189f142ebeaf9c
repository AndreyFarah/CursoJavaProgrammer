	}
	
	
	public static boolean icyHot(int temp1, int temp2) {
		
		//if ((temp1 < 0) && (temp2 > 100) || (temp1 > 0 && temp2 <100)) {
			
			return ((temp1 < 0) && (temp2 > 100) || (temp1 > 0 && temp2 <100));
		//}
	
		  
