package telas;

import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JPanel;

public class TelaMenuPrincipal {
	
	public void telaMenuPrincipal () {
		
		
		String opcao1 = "Digite 1 para Cadastrar";
		String opcao2 = "Digite 2 para sair";
		
		
		JFrame frameMenuPrincipal = new JFrame();
		frameMenuPrincipal.setSize(300,300);
		
		JPanel panelMenuPrincipaJPanel = new JPanel();
		
		JLabel label1 = new JLabel(opcao1);
		panelMenuPrincipaJPanel.add(label1);
		
		JLabel label2 = new JLabel(opcao2);
		panelMenuPrincipaJPanel.add(label2);
		
		
		
		
	}

}
