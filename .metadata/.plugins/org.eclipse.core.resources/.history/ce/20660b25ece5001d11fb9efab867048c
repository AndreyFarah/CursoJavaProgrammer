package warmup1;

public class BackAround {
	
	public static void main (String[] args) {
			
		System.out.println(backAround("a"));
		
	}

	public static String backAround(String str) {
		
		if (str.length() > 1) {
			return  str.substring(str.length()-1) + str + str.substring(str.length()-1);
	}		
		return str;
	}
}
