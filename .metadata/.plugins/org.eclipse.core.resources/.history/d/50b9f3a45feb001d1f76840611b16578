package telas;

import java.util.List;

import javax.swing.JFrame;
import entidades.Convidado;

public class TelaListarConvidado {
	
	public void listarConvidado (List<Convidado> listaConvidado) {
		
		for (Convidado convidado : listaConvidado) {
			System.out.println(convidado.getNome());
			System.out.println(convidado.getEndereco());
			
		}
		
		
		
	}
	
	
}
